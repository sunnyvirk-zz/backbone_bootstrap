{
    "name": "MSDI",
    "version": "0.0.0",
    "description": "Single sign on",
    "main": "app.js",
    "private": "true",
    "scripts": {
        "hooks": "chmod +x hooks/*; cd .git/hooks && ln -sf ../../hooks/* .",
        "lint": "./node_modules/jshint/bin/jshint ./",
        "jscs": "./node_modules/jscs/bin/jscs ./",
        "unit": "./node_modules/mocha/bin/mocha test/unit",
        "feunit": "./node_modules/karma/bin/karma start",
        "feunitbrowser": "node test",
        "test": "npm run lint && npm run jscs && npm run unit && npm run feunit"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Greenfields/mdsi"
    },
    "dependencies": {
        "express": "4.4.1",
        "underscore": "1.6.0",
        "jade": "1.3.1",
        "winston": "0.7.3",
        "jquery": "2.1.1",
        "backbone": "1.1.2"
    },
    "devDependencies": {
        "jshint": "2.5.1",
        "mocha": "1.20.1",
        "chai": "1.9.1",
        "sinon": "1.10.2",
        "sinon-chai": "2.5.0",
        "karma": "0.12.16",
        "karma-mocha": "0.1.4",
        "karma-chrome-launcher": "0.1.4",
        "karma-firefox-launcher": "0.1.3",
        "karma-browserify": "0.2.1",
        "chai-jquery": "1.2.3",
        "gulp": "3.8.2",
        "gulp-jade": "0.6.1",
        "gulp-sass": "0.7.2",
        "gulp-minify-css": "0.3.6",
        "gulp-uglify": "0.3.1",
        "gulp-browserify": "0.5.0",
        "rimraf": "2.2.8",
        "jscs": "1.5.4",
        "enchilada": "0.9.0"
    }
}