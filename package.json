    "backbone": "1.1.2"
  },
  "devDependencies": {
    "jshint": "2.5.1",
    "karma": "0.12.16",
    "karma-chrome-launcher": "0.1.4",
    "karma-firefox-launcher": "0.1.3",
    "karma-browserify": "0.2.1",
    "gulp": "3.8.2",
    "gulp-jade": "0.6.1",
    "gulp-sass": "0.7.2",
    "gulp-minify-css": "0.3.6",
    "gulp-uglify": "0.3.1",
    "gulp-browserify": "0.5.0",
    "rimraf": "2.2.8",
    "jscs": "1.5.4",
    "enchilada": "0.9.0",
    "karma-jasmine": "^0.1.5",
    "sinon": "^1.10.2",
    "jasmine-sinon": "^0.4.0",
    "jasmine-jquery": "^1.3.3"
  }
